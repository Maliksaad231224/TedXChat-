
{
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    }
  ]
}
